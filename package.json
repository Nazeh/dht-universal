{
  "name": "dht-universal",
  "version": "0.0.1",
  "description": "Universal wrapper for @hyperswarm/dht and @hyperswarm/dht-relay working in Node.js and the Browser",
  "type": "module",
  "keywords": [
    "dht",
    "hyperswarm",
    "dht-relay",
    "dht-universal",
    "universal",
    "nodejs",
    "browser"
  ],
  "homepage": "https://github.com/synonymdev/dht-universal#readme",
  "bugs": {
    "url": "https://github.com/synonymdev/dht-universal/issues"
  },
  "license": "MIT",
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "types": "types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "types/*",
        "types/src/*"
      ],
      "types/*": [
        "types/*",
        "types/src/*"
      ]
    }
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synonymdev/dht-universal.git"
  },
  "scripts": {
    "build": "aegir build",
    "test": "aegir build && c8 aegir test",
    "lint": "aegir ts -p check && aegir lint --fix",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check"
  },
  "dependencies": {
    "@hyperswarm/dht": "github:hyperswarm/dht#v5.0.11",
    "@hyperswarm/dht-relay": "github:hyperswarm/dht-relay"
  },
  "devDependencies": {
    "aegir": "^36.1.3",
    "dht-universal": "file:dist"
  },
  "peerDependencies": {
    "b4a": "^1.3.1"
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "exports": {
    ".": {
      "browser": "./src/dht.browser.js",
      "import": "./src/dht.js"
    }
  }
}